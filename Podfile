# platform :ios, '9.0'

target 'firebase-sample' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for firebase-sample
  pod 'FirebaseUI'
  pod 'Firebase/Crashlytics'

  # Google Analyticsの機能を利用するために推奨されています
  pod 'Firebase/Analytics'

end
